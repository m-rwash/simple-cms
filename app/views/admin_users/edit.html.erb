<% @page_title = "Update "+@admin_user.first_name+" Page" %>
<%= link_to("Back to Admin Users List", admin_users_path) %>
<h2>Update Admin User: <%=@admin_user.first_name%></h2>
<div>
	<%= form_for(@admin_user, :url => admin_user_path(@admin_user)) do |f| %>
		<%= render(:partial => 'form', :locals => {:f => f}) %>
		<div class="form-buttons">
	      <%= f.submit("Update") %>
	    </div>
	<% end %>
</div>
