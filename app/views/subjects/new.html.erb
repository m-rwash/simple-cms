<% @page_title = "New Subject" %>
<%= link_to("<< Back to List", subjects_path) %>

<div>
	<h2>Create Subject</h2>

	<%= form_for(@subject, :url => subjects_path, :method => 'post') do |f| %>
		<%= render(:partial => 'form', :locals => {:f => f}) %>
		<div>
			<%= f.submit("Create Subject")%>
		</div>
	<%end%>
</div>